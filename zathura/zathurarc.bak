### Settings from original zathurarc ###
set guioptions "s v"
set window-height 1000
set window-width 1500

### zathura-pywal ###
# Allow recolor
set recolor 'true'

# Don't allow original hue when recoloring
set recolor-keephue 'false'

# Don't keep original image colors while recoloring
set recolor-reverse-video 'false'

# Command line completion entries
set completion-fg '#c7daa1'
set completion-bg '#0d0f0b'

# Command line completion group elements
set completion-group-fg '#6FA755'
set completion-group-bg '#0d0f0b'

# Current command line completion element
set completion-highlight-fg '#0d0f0b'
set completion-highlight-bg '#c7daa1'

# Default foreground/background color
set default-bg rgba(13,15,11,1)

# Input bar
set inputbar-fg '#c7daa1'
set inputbar-bg '#0d0f0b'

# Notification
set notification-fg '#c7daa1'
set notification-bg '#0d0f0b'

# Error notification
set notification-error-fg '#c7daa1'
set notification-error-bg '#69982B'

# Warning notification
set notification-warning-fg '#c7daa1'
set notification-warning-bg '#69982B'

# Tab - TODO
# set tabbar-fg
# set tabbar-bg

# Focused tab - TODO
# set tabbar-focus-fg
# set tabbar-focus-bg

# Status bar
set statusbar-fg '#c7daa1'
set statusbar-bg '#0d0f0b'

# Highlighting parts of the document (e.g. show search results)
set highlight-color '#6FA755'
set highlight-active-color '#6FA755'

# Represent light/dark colors in recoloring mode
set recolor-lightcolor rgba(0,0,0,0)
set recolor-darkcolor '#c7daa1'

# 'Loading...' text
set render-loading-fg '#c7daa1'
set render-loading-bg '#0d0f0b'

# Index mode
set index-fg '#c7daa1'
set index-bg '#0d0f0b'

# Selected element in index mode
set index-active-fg '#0d0f0b'
set index-active-bg '#c7daa1'